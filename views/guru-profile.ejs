<div class="page-header">
    <h1><%= guru.name %></h1>
    <a href="/guru-edit" class="btn btn-default btn-sm">Edit Profile</a>
</div>
<div class="row">
    <div class="col-sm-6 col-sm-offset-3">
        <div class="well">
            <p>
                <strong>Name</strong>: <%= guru.name %><br>
                <strong>id</strong>: <%= guru._id %><br>
                <strong>email</strong>: <%= guru.email %><br>
                <strong>password</strong>: <%= guru.password %><br>
                <strong>activities</strong>: <%= guru.activities %><br>
                <strong>studio</strong>: <%= guru.studio %><br>
                <strong>reviews</strong>: <%= guru.reviews %><br>
                <strong>Image</strong>: <%= guru.picture %><br>
                
            </p>
                
        </div>
    </div>
</div>

<script src="/js/app.js"></script>