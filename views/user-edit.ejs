<div class="page-header">

    <h1><%= user.name %></h1>

    <a href="/user-logout" class="btn btn-default btn-sm">Logout</a>

</div>


<form>
    <div class="form-group">
            <label for="exampleInputNamel1">Name</label>
            <input type="name" class="form-control" id="user-name" aria-describedby="nameHelp" placeholder="Enter name">
            <small id="NameHelp" class="form-text text-muted">Our Member</small>
    </div>

  
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <input type="email" class="form-control" id="user-email" aria-describedby="emailHelp" placeholder="Enter email">
      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Password</label>
      <input type="password" class="form-control" id="user-password" placeholder="Password">
    </div>

    <div class="form-group">
            <label for="exampleInputZipl1">Zip Code</label>
            <input type="zipcode" class="form-control" id="user-zip" aria-describedby="zipcodeHelp" placeholder="Zip Code">
            <small id="ZipCodeHelp" class="form-text text-muted"></small>
    </div>

    <div class="form-group">
            <input type="radio" name="sex" value="male" checked> Male
            <br>
            <input type="radio" name="sex" value="female"> Female
            <br>
            <input type="radio" name="sex" value="intersex"> Intersex
            <br>
            <br>
    </div>
    
    <!-- <div class="form-check">
      <input type="checkbox" class="form-check-input" id="exampleCheck1">
      <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>-->


    <button type="submit" class="btn btn-primary">Submit</button>
  </form>


  <script src="/js/app-user.js"></script>